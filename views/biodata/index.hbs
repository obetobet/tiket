

{{>head}}
{{>navbar}}

<div class="row">
    <div class="col-12">
        <div class="card-box table-responsive">

            <p class="text-muted font-13 m-b-40" style="margin-left:15px">
                <a href="http://localhost:3000/biodata/create" class="btn btn-sm btn-success ">Add new</a>
            </p>

            <table id="datatable-buttons" class="table table-striped table-bordered" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Name</th>
                        <th>Alamat</th>
                        <th>Action</th>
                    </tr>
                    </thead>
                    <tbody>
                        {{#each data}}
                        <tr>
                            <td>{{ @index }}</td>
                            <td>{{ nama }}</td>
                            <td>{{ alamat }}</td>
                        <td>
                            
                                <a href="/informasi/{{id}}/edit" class="btn btn-sm btn-info edit">Edit</a>
                            <form>
                            <a href="javascript:void(0);" class="btn btn-sm btn-danger delete">Delete</a>
                            </form>
                                


                        </td>
                    </tr>
                    {{/each}}

                </tbody>
            </table>
        </div>
    </div>
</div>
<!-- end row -->

</div>
{{>footer}}